{
  "name": "@lovart-open/flags",
  "version": "0.0.3",
  "description": "Feature flag management with multi-layer priority system (client-side)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./statsig": {
      "types": "./dist/statsig/index.d.ts",
      "import": "./dist/statsig/index.js",
      "default": "./dist/statsig/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "feature-flag",
    "statsig",
    "parameter-store"
  ],
  "author": "huxingyu@liblib.ai",
  "license": "MIT",
  "dependencies": {
    "@statsig/js-client": "^3.30.0",
    "@statsig/react-bindings": "^3.30.2",
    "lodash-es": "^4.17.23",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.0.0 || ^19.0.0",
    "jsdom": "^23.0.1",
    "tsup": "^8.5.1",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lovartai/flags.git"
  }
}
